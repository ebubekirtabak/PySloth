{
  "settings": {
    "search_time_sleep": 5,
    "download_time_sleep": 5,
    "thread_time_out": 300,
    "thread_limit": 15,
    "thread_controller": "mongo",
    "session_id": "linkedin",
    "role": "main",
    "is_go_again_history": false,
    "is_page_helper": false,
    "time_out": 0,
    "multi_process": {
      "time_out": 300,
      "limit": 10,
      "base": "mongo"
    },
    "driver": {
      "driver_type": "chrome",
      "driver_path": "/usr/local/bin/chromedriver-85",
      "driver_arguments": [
      ]
