    "user_agent": "Mozilla/5.0 (Windows NT 6.2; WOW64; Trident/7.0; rv:11.0) like Gecko",
    "accept_language": "en-US,en;q=0.5",
    "data": null
  },
  "scope_name": "amazon_product",
  "history": {
    "to": "database",
    "collection_name": "history_collection",
    "control": {
      "from": "database",
      "progress": "skip"
    }
  },
  "isPagination": true,
  "isLogger": true,
  "before_actions": [
  ],
  "script_actions": [
    {
      "type": "$_GET_VARIABLE",
      "variable_name": "price",
      "attribute_name": "text",
      "selector": "//*[@id=\"priceblock_ourprice\"]"
    },
    {
      "type": "$_GET_VARIABLE",
      "variable_name": "product_title",
      "attribute_name": "text",
      "selector": "//*[@id=\"productTitle\"]"
    },
    {
      "type": "parse_html_list",
      "variable_name": "accessory",
      "selector": "//*[@id=\"simpleBundleV2_feature_div\"]/div/div[1]/div/div/span",
      "object_list": [
        {
          "selector": ".//div/label/span/span[2]",
          "attribute_name": "text",
          "type": "$_GET_VARIABLE",
          "variable_name": "price"
        },
        {
          "selector": ".//div/label/span/span[1]/a",
          "attribute_name": "text",
          "type": "$_GET_VARIABLE",
          "variable_name": "name"
        }
      ]
    },
    {
      "type": "event",
      "delay": 4,
      "sleep": 1,
      "actions": [
        {
          "type": "nothing"
        }
      ]
    }
  ]
